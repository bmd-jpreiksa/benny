				"box" : 				{
					"code" : "Buffer prm(\"voice_parameter_buffer\");\nBuffer changed_flags(\"changed_flags\");\n\n// main parameters\nHistory ti,mu,ar,cu,lo,ve,ac,i1,i2,o2,cu_t(0),cu_a(0),cu_f(0),ga;\n\n// state\nHistory ov(0);\nHistory oov(0);\nHistory otrig(0);\nHistory otr(0);\nHistory env(0);\nHistory attdone(1);\nHistory decdone(1);\nHistory target(0);\nHistory attrate(0.00001);\nHistory decrate(0.00001);\nHistory looptime(0);\nHistory looptimer(0);\nHistory upda(0);\n\n// tap measurement state\nHistory tap_timer_s(0);       // samples since last tap\nHistory last_tap_ms(500);     // measured tap interval (ms)\nHistory have_tap(0);          // 0 until at least one tap\nParam samplerate(48000);      // set this to your actual SR\n\n// envelope total time\nHistory total_time(0);\n\nParam voice_offset(0);\nParam voice_is(0);\nParam tr(0);\nParam v(0);\nParam panic(0);\n\n// ---- parameter update ----\nif (upda <= 0) {\n    upda = vectorsize + 4;\n    if (peek(changed_flags, voice_is, 0) > 0) {\n        changed_flags.poke(0, voice_is);\n        ti = peek(prm, voice_offset, 0, channels=1);\n        mu = peek(prm, 1+voice_offset, 0, channels=1);\n        ar = peek(prm, 2+voice_offset, 0, channels=1);\n        ar = clip(ar, 0.001, 0.999);\n        cu = peek(prm, 3+voice_offset, 0, channels=1);\n        lo = peek(prm, 4+voice_offset, 0, channels=1);\n        ve = peek(prm, 5+voice_offset, 0, channels=1)*2 - 1;\n        ac = peek(prm, 6+voice_offset, 0, channels=1)*2 - 1;\n        i1 = floor(5*peek(prm, 7+voice_offset, 0, channels=1));\n        i2 = floor(3*peek(prm, 8+voice_offset, 0, channels=1));\n        o2 = floor(4*peek(prm, 9+voice_offset, 0, channels=1));\n        ga = 2*peek(prm, 10+voice_offset, 0, channels=1)-1;\n        same = peek(prm, 11+voice_offset, 0, channels=1)>0.5;\n        cu_f=cu*4;\n        cu_t=floor(cu_f);\n        cu_f-=cu_t;\n        cu_t+=1;\n        cu_a = same ? cu_t : (5-cu_t);\n        t=1+abs(cu-0.45)*23;\n    }\n}\nupda -= 1;\n\n// ---- tap measurement ----\n// increment sample counter\ntap_timer_s += 1;\n\n// detect rising edge on in3 (tap input pulse assumed 0..1)\ntap_edge = delta(in3 > 0.5) > 0;\nif (tap_edge) {\n    tap_ms = tap_timer_s * (1000 / samplerate);  // convert samples → ms\n    tap_ms = clip(tap_ms, 20, 4000);             // clamp to 20ms–4s\n    alpha = 0.2;                                 // smoothing\n    last_tap_ms = alpha*tap_ms + (1-alpha)*last_tap_ms;\n    have_tap = 1;\n    tap_timer_s = 0;                             // reset like reset_tapintmr()\n}\n\n// ---- compute envelope times ----\nmu_scale = max(0.000001, mu);\n\n// tap ratio table\ntap_selector = floor(clip(ti, 0, 1) * 18 + 0.5);\ntap_selector = clip(tap_selector, 0, 18);\ntap_ratio = selector(\n    tap_selector,\n    0.0625,0.0833333,0.125,0.1666667,0.25,0.3333333,0.5,0.6666667,1,\n    1.3333333,2,4,8,12,16,20,24,28,32\n);\n\n// decide total_time\nif (have_tap) {\n    total_time = last_tap_ms * tap_ratio * mu_scale;\n} else {\n    total_time = (pow(1000, ti)-1+1e-10)*60.06006006 * mu_scale;\n}\n\n// attack/decay split\nat = total_time * ar;\nde = total_time * (1 - ar);\nattrate = 1 / mstosamps(max(0.001, at));\ndecrate = -1 / mstosamps(max(0.001, de));\nlooptime = mstosamps(total_time);\n\nif ((looptimer <= 0) && (lo)) {\n    ov = ov*lo;\n    if (ov > 0.001) {\n        attdone=0;\n        decdone=1;\n        target=ov*lo;\n        looptimer=looptime;\n    }\n}\n\n// ---- trigger logic ----\nvi=0;\nlevscale=0;\ntrig=0;\n\n// accept trigger edges regardless of attdone\nif (delta(tr)) trig += tr;\n\n// loop retrigger from lo input\nif ((delta(lo > 0) > 0) && (env == 0)) {\n    trig = 1;\n}\n\natr = attrate;\nder = decrate;\n\nif (i1==0) {\n    trig += (in1 > 0.00000001);\n    levscale += trig;\n} else if (i1==1) {\n    levscale += trig;\n    trig = 0;\n    otrig=0;\n    vi += in1;\n    cu_t = clip(cu_t,2,3);\n    cu_f = clip(cu_f,0.001,0.999);\n} else if (i1==2) {\n    atr /= (1+in1);\n} else if (i1==3) {\n    der /= (1+in1);\n} else if (i1==4) {\n    atr /= (1+in1);\n    der /= (1+in1);\n}\nif (in2 != 0) {\n    if (i2==0) {\n        atr /= (1+in2);\n    } else if (i2==1) {\n        der /= (1+in2);\n    } else if (i2==2) {\n        atr /= (1+in2);\n        der /= (1+in2);\n    }\n}\nif (panic) {\n    target=0;\n    decdone=0;\n    der=-0.001;\n    env*=0.999;\n}\n\nif (v > 0) {\n    levscale = v * ve + (1-clip(ve,0,2));\n    trig += levscale;\n}\nif (vi != oov) {\n    oov=vi;\n    ov=vi;\n    target=vi;\n    if (target < env) {\n        attdone=1;\n        decdone=0;\n    } else if (target > env) {\n        decdone=1;\n        attdone=0;\n    }\n}\n\n// retrigger envelope whenever trig changes\nif (trig != otrig) {\n    if (trig != 0) {\n        attdone=0;\n        decdone=1;\n        otrig=trig;\n        ov = levscale + ac*env;\n        ov /= 2;\n        ov = clip(ov,-2,2);\n        ov = ov - (ov*ov*ov/3);\n        ov *= 2;\n        target=ov;\n        looptimer=looptime;\n    } else {\n        trig = 0;\n        otrig = 0;\n    }\n}\n\n// ---- envelope generator ----\nif (!attdone) {\n    t=clip(target-env,-0.9999,0.9999);\n    if (t > 0.00001) {\n        dx = selector(cu_a,\n            (1-t)*(1-t*cu_f),\n            1+t*(cu_f-1),\n            1+cu_f*(t-1),\n            (t+0.01)*(1+cu_f*(t-0.99))\n        );\n        dx *= atr;\n        dx = clip(dx, 0.0000001, t);\n        env += dx;\n    } else {\n        env = target;\n        attdone=1;\n        if (i1 != 1) {\n            decdone=0;\n            target=0;\n            trig=0;\n        }\n    }\n}\nif (!decdone) {\n    t=clip(target-env,-0.9999,0.9999);\n    if (t < -0.001) {\n        dx = selector(cu_t,\n            ((t+1)*(t+1)-1)*(1-cu_f)+cu_f*t,\n            t-cu_f*(t+1),\n            -1-cu_f*t,\n            -1-t*(1+cu_f+cu_f*t)\n        );\n        dx *= -der;\n        dx = clip(dx, t, -0.0000000001);\n        env += dx;\n    } else {\n        env=target;\n        decdone=1;\n    }\n}\n\nlooptimer -= 1;\n\n// ---- outputs ----\nout1 = ga*env;\n\nif (o2==0) {\n    out2 = target;\n} else if (o2==1) {\n    out2 = attdone;\n} else if (o2==2) {\n    out2 = decdone;\n} else {\n    out2 = delta(env);\n}\n\nout3 = attdone;\nout4 = decdone;\nout5 = (env < 0.0001) * (attdone) * (lo==0) * (in1==0) * (in2==0);\n",
